<Window x:Class="_221_Globa.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_221_Globa"
        mc:Ignorable="d"
                    Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="45"></RowDefinition>
            <RowDefinition>

            </RowDefinition>
            <RowDefinition Height="45"></RowDefinition>
        </Grid.RowDefinitions>
        <Frame x:Name="MainFrame" Grid.Row="1" Source="Pages/AuthPage.xaml" Navigated="MainFrame_Navigated" NavigationUIVisibility="Hidden"/>
        <TextBlock x:Name="TB_Time" HorizontalAlignment="Right" VerticalAlignment="Center" Grid.Row="2" Height="20" Width="150" Margin="0,0,20,0" />
        <Button x:Name="But_return"  Content="Назад" HorizontalAlignment="Left"  Margin="20,0,0,0" VerticalAlignment="Center" Width="100" Click="But_return_Click"/>
    </Grid>
</Window>